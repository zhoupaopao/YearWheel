<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <com.example.a13126.yearwheel.PullToRefreshLayout
        android:id="@+id/refresh_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include layout="@layout/refresh_head"/>
    <com.example.a13126.yearwheel.pullableview.PullableListView
        android:id="@+id/YWlist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/ios_btntext_blue"></com.example.a13126.yearwheel.pullableview.PullableListView>
        <include layout="@layout/load_more"/>
    </com.example.a13126.yearwheel.PullToRefreshLayout>

</LinearLayout>